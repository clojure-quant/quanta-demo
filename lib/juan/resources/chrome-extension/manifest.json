{"manifest_version": 3,
 "name": "sentiment-cookie-grabber",
 "description": "grabs cookies to use sentiment site",
 "version": "1.0",
 //"background": {
 //  "scripts": [ "chrome.js" ]
 //},
 //"browser_action": {
 //   "default_icon": "icon.png"
 //},
 "permissions": [ "webRequest",
                  //"webRequestBlocking", 
                  "webNavigation", 
                  "tabs", 
                  //"<all_urls>", 
                  "debugger",
                  "cookies", 
                  "contextMenus"
                ],
 "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icon.png",
    "32": "icon.png"}},
 //;background.service_worker
 "host_permissions": [
    "*://*.google.com/",
    "*://*.myfxbook.com/",
    "*://localhost:8080/",
    "*://*.quant.hoertlehner.com/"
 ]
}

